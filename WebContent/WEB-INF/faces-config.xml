<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>enlace_listado</from-outcome>
			<to-view-id>/pages/enlace_listado.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>enlace_mantenimiento</from-outcome>
			<to-view-id>/pages/enlace_mantenimiento.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>docente_listado</from-outcome>
			<to-view-id>/pages/docente_listado.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>docente_mantenimiento</from-outcome>
			<to-view-id>/pages/docente_mantenimiento.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>rol_listado</from-outcome>
			<to-view-id>/pages/rol_listado.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>rol_mantenimiento</from-outcome>
			<to-view-id>/pages/rol_mantenimiento.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>
	
</faces-config>
